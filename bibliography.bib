@article{xingParticipationbasedStudentFinal2015,
  title = {Participation-Based Student Final Performance Prediction Model through Interpretable {{Genetic Programming}}: {{Integrating}} Learning Analytics, Educational Data Mining and Theory},
  shorttitle = {Participation-Based Student Final Performance Prediction Model through Interpretable {{Genetic Programming}}},
  author = {Xing, Wanli and Guo, Rui and Petakovic, Eva and Goggins, Sean},
  year = {2015},
